{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tt-audio-lab",
  "version": "0.1.0",
  "identifier": "com.tt.audio.lab",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "tt-audio-lab",
        "width": 1200,
        "height": 760,
        "resizable": true,
        "decorations": true,
        "transparent": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}
